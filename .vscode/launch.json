  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Launch Brave",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5500",
      "webRoot": "${workspaceFolder}",
      "runtimeExecutable": "C:\\Users\\OS-MASTER\\AppData\\Local\\BraveSoftware\\Brave-Browser\\Application\\brave.exe"
    }


]
}